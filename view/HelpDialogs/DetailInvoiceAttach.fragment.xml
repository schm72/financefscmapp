			<Text text="{ArcDocId}" wrapping="false"/>
			<Text text="{ path: 'ArDate', formatter: 'com.springer.springerfscmapp.util.Formatter.dateSimple2' }" wrapping="false"/>
			<Text text="{Reserve}" wrapping="false"/>
			<Text text="{ArObject}" wrapping="false"/>
		</cells>
	</ColumnListItem>
</core:FragmentDefinition>