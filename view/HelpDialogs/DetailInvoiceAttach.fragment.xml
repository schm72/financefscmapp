<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns="sap.m">
	<ColumnListItem press="handleSelectionChange" type="{UIModel>/itemType}">
		<cells>
			<Text text="{Belnr}_{Gjahr}.{Reserve}" wrapping="false"/>
			<Text text="{ path: 'ArDate', formatter: 'com.springer.financefscmapp.util.Formatter.dateSimple2' }" wrapping="false"/>
			<Text text="{Reserve}" wrapping="false"/>
			<Text text="{ArObject}" wrapping="false"/>
		</cells>
	</ColumnListItem>
</core:FragmentDefinition>